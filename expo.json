{
  "expo": {
    "name": "TidKoll",
    "slug": "tidkoll-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tidkoll.app"
    },
    "android": {
      "package": "com.tidkoll.app",
      "icon": "./assets/icon.png"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
